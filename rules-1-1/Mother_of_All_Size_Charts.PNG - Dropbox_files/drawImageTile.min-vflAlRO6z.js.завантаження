define(["require","exports","file-viewer/watermarking/plugin/utils/toRadAngle"],(function(a,t,e){"use strict";Object.defineProperty(t,"__esModule",{value:!0}),t.drawImageTile=void 0,t.drawImageTile=function(a){var t=a.image,n=a.scale,i=a.opacity,r=a.angle,h=e.toRadAngle(r),o=document.createElement("canvas"),s=o.getContext("2d");if(!s)throw new Error("Cannot draw on canvas");var c=t.width*n,d=t.height*n;o.width=Math.round(Math.abs(c*Math.cos(h))+Math.abs(d*Math.sin(h))),o.height=Math.round(Math.abs(c*Math.sin(h))+Math.abs(d*Math.cos(h)));var M=0,l=0;return r<90?l=c*Math.sin(h):r<180?(M=-c*Math.cos(h),l=c*Math.sin(h)-d*Math.cos(h)):r<270?(M=-c*Math.cos(h)-d*Math.sin(h),l=-d*Math.cos(h)):M=-d*Math.sin(h),s.globalAlpha=i,s.translate(M,l),s.rotate(-h),s.drawImage(t,0,0,c,d),{canvas:o,actualUnrotatedHeight:d,actualUnrotatedWidth:c}}}));
